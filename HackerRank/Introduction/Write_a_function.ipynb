        "def is_leap(year):\n",
        "\n",
        "    # Write your logic here\n",
        "    if (year %4 == 0) and (year %100 != 0):\n",
        "        leap = True\n",
        "    elif (year % 400 == 0) and (year % 100 == 0):\n",
        "        leap = True\n",
        "    else:\n",
        "        leap = False\n",
        "    \n",
        "    return leap\n",
        "\n",
        "    \n",
        "year = int(input())\n",
        "print(is_leap(year))"
